<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="helloWorld" class="com.ErfAn.HelloWorld" destroy-method="destroy" init-method="init">
<property name="message" value="Hello World!"></property>
</bean>

<bean id="myhelloWorld" class="com.ErfAn.MyHelloWorld" destroy-method="destroy" init-method="init">
<property name="message" value="MyHello World!"></property>
</bean>

<bean class="com.ErfAn.Hello2World"></bean>
</beans>


